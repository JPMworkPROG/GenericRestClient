{
   "ApiClient": {
      "BaseUrl": "https://jsonplaceholder.typicode.com/",
      "Authentication": {
         "Type": "OAuth2",
         "ClientId": "abc123",
         "ClientSecret": "xyz456",
         "TokenEndpoint": "https://auth.exemplo.com/oauth2/token",
